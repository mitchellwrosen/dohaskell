ul .nav {
   list-style-type: none;
   margin: 0;
   padding: 0;
   overflow: hidden;
}

li .nav {
   float: left;
}

a .nav :link, a .nav :visited {
   display: block;
   width: 120px;
   font-weight: bold;
   color: #FFFFFF;
   background-color: #98bf21;
   text-align: center;
   padding: 4px;
   text-decoration: none;
   text-transform: uppercase;
}
